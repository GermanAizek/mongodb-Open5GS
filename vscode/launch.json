{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch EloqDoc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/mongo/mongod",
            "args": [
                "--config",
                "src/mongo/db/modules/eloq/config/standalone/debug_coroutine.conf"
                // "src/mongo/db/modules/eloq/config/multinode/node2.template.conf"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // path to libtxservice.so, liblogservice.so
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/src/mongo/db/modules/eloq/build"
                },
                {
                    // preload asan library and libbraft.so(libbrpc.so)
                    // for more information, please refer to `src/mongo/db/modules/eloq/issues.md`
                    "name": "LD_PRELOAD",
                    "value": "/usr/lib/x86_64-linux-gnu/libasan.so.5:/usr/local/lib/libmimalloc.so:/usr/lib/libbrpc.so"
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
            ],
            "preLaunchTask": "clean",
            "postRemoteConnectCommands": [
                {
                    "description": "privided by mongodb",
                    "text": "source .gdbinit",
                    "ignoreFailures": false
                }
            ]
        },
        {
            "name": "(gdb) Launch eloq RelWithDebInfo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/RelWithDebInfo/mongo/mongod",
            "args": [
                "--config",
                "src/mongo/db/modules/eloq/test_folder/config/eloq/release_coroutine.conf"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // path to libtxservice.so, liblogservice.so
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/src/mongo/db/modules/eloq/build"
                },
                {
                    // preload asan library and libbraft.so(libbrpc.so)
                    // for more information, please refer to `src/mongo/db/modules/eloq/issues.md`
                    "name": "LD_PRELOAD",
                    "value": "/usr/lib/libbrpc.so"
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
            ],
            "preLaunchTask": "clean",
            "postRemoteConnectCommands": [
                {
                    "description": "privided by mongodb",
                    "text": "source .gdbinit",
                    "ignoreFailures": false
                }
            ]
        },
        {
            "name": "(gdb) Launch wiredtiger",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/mongo/mongod",
            "args": [
                "--storageEngine",
                "wiredTiger",
                "--dbpath",
                "${workspaceFolder}/src/mongo/db/modules/eloq/test_folder/test_wiredtiger",
                "--logpath",
                "${workspaceFolder}/src/mongo/db/modules/eloq/test_folder/log/log.json",
                "--setParameter",
                "logComponentVerbosity={storage: 1}",
                "--setParameter",
                "diagnosticDataCollectionEnabled=false",
                "--setParameter",
                "disableLogicalSessionCacheRefresh=true",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    // path to libtxservice.so, liblogservice.so
                    "name": "LD_LIBRARY_PATH",
                    "value": "${workspaceFolder}/src/mongo/db/modules/eloq/build"
                },
                {
                    // preload asan library and libbraft.so(libbrpc.so)
                    // for more information, please refer to `src/mongo/db/modules/eloq/issues.md`
                    "name": "LD_PRELOAD",
                    "value": "/usr/lib/x86_64-linux-gnu/libasan.so.5:/usr/lib/libbraft.so"
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                },
            ],
            "postRemoteConnectCommands": [
                {
                    "description": "privided by mongodb",
                    "text": "source .gdbinit",
                    "ignoreFailures": false
                }
            ]
        }
    ]
}